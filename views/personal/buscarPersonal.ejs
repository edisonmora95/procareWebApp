<!DOCTYPE html>
<html>
	<head>
		<% include ../partials/head %>
		<link rel="stylesheet" type="text/css" href="/stylesheets/procarianos/procarianosStyles.css">
		<link rel="stylesheet" type="text/css" href="/stylesheets/procarianos/buscarProcarianoStyles.css">
	</head>
	<body>
		<div>
			<header>
				<% include ../partials/navbar %>
			</header>
			<main class="container"  id="main">
				<h3 class="center-align">Personal</h3>
				<!--Resultados-->
				<section id="section-resultados">
					<div class="row" id="row-resultados">
						<table class="stripped bordered responsive-table centered highlight">
							<thead>
								<th>Nombre</th>
								<th>Apellido</th>
								<th>CÃ©dula</th>
								<th>Fecha de nacimiento</th>
								<th>Email</th>
								<th>Convencional</th>
								<th>Celular</th>
								<th>Tipo</th>
					
							</thead>
							<tbody>
								<tr v-for="personal in arregloPersonal">
									<td @click="irAPerfil(personal)"><a  class="nombre">{{personal.nombres}}</a></td>
									<td>{{personal.apellidos}}</td>
									<td>{{personal.cedula}}</td>
									<td>{{date(personal.fechaNacimiento)}}</td>
									<td>{{personal.email}}</td>
									<td>{{personal.convencional}}</td>
									<td>{{personal.celular}}</td>
									<td>{{personal.tipo}}</td>
								</tr>
							</tbody>
						</table>
					</div>
				</section>
			</main>
		</div>
		<div>
			<% include ../partials/scripts %>
			<script src="/scripts/moment/moment.js"></script>
			<script src="/scripts/moment/locale/es.js"></script>
			<script src="/build/personal/tablaPersonalScript.min.js"></script>
		</div>
	</body>
</html>